<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="663.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.casyu.tinynotepad.settings.SettingsController">
    <left>
        <TreeView fx:id="settingsTree" prefWidth="150.0" showRoot="false" BorderPane.alignment="CENTER">
            <root>
                <TreeItem value="Settings">
                    <children>
                        <TreeItem value="常规" />
                        <TreeItem value="外观" />
                    </children>
                </TreeItem>
            </root>
        </TreeView>
    </left>
    <center>
        <StackPane fx:id="settingsPane" />
    </center>
</BorderPane>
